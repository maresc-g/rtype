server -> "PLAYER %id;%numPlayer;%pos" UDP
server -> "SCROLL %pos" UDP
server -> "DEAD %id" TCP

server -> LEVELUP %lvl TCP
server -> "MAP %map" TCP map=<idObject;posx;posy>


